int arr[] = new int[2];
		HashMap<Integer, Integer> hm = new HashMap<Integer, Integer>();
		for (int i = 0; i < nums.length; i++) {
			if (hm.get(target - nums[i]) == null) {
				hm.put(nums[i], i + 1);
			} else {
				arr[0] = hm.get(target - nums[i]);
				arr[1] = i + 1;
				break;
			}

		}
		return arr;
    }
